<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uoregon.cnf.tidetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".SelectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--<activity android:name=".MainActivity"></activity>-->
        <activity
            android:name=".ViewTidesActivity"
            android:label="View Tide Predictions"
            android:parentActivityName=".SelectionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.uoregon.cnf.tidetracker.SelectionActivity"/>
        </activity>
    </application>

</manifest>